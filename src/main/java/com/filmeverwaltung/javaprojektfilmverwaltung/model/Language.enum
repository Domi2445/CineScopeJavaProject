package com.filmeverwaltung.javaprojektfilmverwaltung.model;

public enum Language {

    DE("Deutsch"),
    EN("English"),
    AR("العربية"),
    PL("Polski");

    private final String displayName;

    Language(String displayName) {
        this.displayName = displayName;
    }

    @Override
    public String toString() {
        return displayName;
    }
}
