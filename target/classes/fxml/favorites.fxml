<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.filmeverwaltung.javaprojektfilmverwaltung.controller.FavoritesController"
            prefHeight="400.0" prefWidth="600.0">

    <TableView fx:id="tableFavorites" layoutX="20" layoutY="20" prefWidth="560" prefHeight="360">
        <columns>
            <TableColumn fx:id="colTitle" text="Titel" prefWidth="250"/>
            <TableColumn fx:id="colYear" text="Jahr" prefWidth="100"/>
            <TableColumn fx:id="colRating" text="Bewertung" prefWidth="100"/>
        </columns>
    </TableView>

</AnchorPane>
